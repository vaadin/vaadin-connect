{
  "functionalSuites": "e2e/**/*.spec.*",
  "serverUrl": "http://localhost:8080",
  "serverPort": 9002,
  "socketPort": 9003,
  "environments": ["chrome"],
  "browser": {
    "loader": {
      "script": "esm"
    }
  },
  "node": {
    "suites": "frontend/test/**/*.spec.*",
    "plugins": [
      "./frontend/test/utils.js"
    ]
  },
  "instrumenterOptions": {
    "esModules": true
  },
  "tunnelOptions": {
    "version": "3.141.59",
    "drivers": [
      {"name": "chrome", "version": "75.0.3770.140"},
      {"name": "firefox", "version": "0.24.0"}
    ]
  },
  "configs": {
    "sauce": {
      "tunnel": "saucelabs",
      "environments": [
        {
          "platform": "Windows 10",
          "browserName": "chrome",
          "version": [
            "latest-1",
            "latest"
          ],
          "extendedDebugging": true
        },
        {
          "platform": "Windows 10",
          "browserName": "firefox",
          "version": [
            "latest-1",
            "latest"
          ],
          "extendedDebugging": true
        },
        {
          "platform": "Windows 10",
          "browserName": "MicrosoftEdge",
          "version": [
            "latest-1",
            "latest"
          ],
          "extendedDebugging": true
        },
        {
          "platform": "Windows 10",
          "browserName": "internet explorer",
          "version": "11",
          "extendedDebugging": true,
          "fixSessionCapabilities": false
        },
        {
          "platform": "OS X 10.13",
          "browserName": "safari",
          "version": [
            "11"
          ],
          "extendedDebugging": true
        },
        {
          "platform": "OS X 10.13",
          "browserName": "iphone",
          "version": [
            "11.3",
            "12.0"
          ],
          "extendedDebugging": true
        },
        {
          "platform": "Linux",
          "browserName": "android",
          "version": "6.0",
          "extendedDebugging": true
        }
      ]
    },
    "ci-public": {},
    "ci": {
      "extends": ["sauce"]
    },
    "debug-chrome": {
      "environments": {
        "browserName": "chrome",
        "fixSessionCapabilities": false,
        "goog:chromeOptions": {
          "debuggerAddress": "127.0.0.1:9222"
        }
      }
    }
  }
}
