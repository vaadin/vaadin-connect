{
  "functionalSuites": "e2e/**/*.spec.js",
  "serverUrl": "http://localhost:8081",
  "serverPort": 9002,
  "socketPort": 9003,
  "environments": [
    {
      "browserName": "chrome",
      "fixSessionCapabilities": false
    }
  ],
  "tunnelOptions": {
    "version": "3.141.59",
    "drivers": [
      {"name": "chrome", "version": "2.44"},
      {"name": "firefox", "version": "0.21.0"}
    ]
  },
  "configs": {
    "sauce": {
      "tunnel": "saucelabs",
      "environments": [
        {
          "platform": "Windows 10",
          "browserName": "chrome",
          "version": ["latest-1", "latest"],
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "Windows 10",
          "browserName": "firefox",
          "version": ["latest-1", "latest"],
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "Windows 10",
          "browserName": "MicrosoftEdge",
          "version": ["latest-1", "latest"],
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "Windows 10",
          "browserName": "internet explorer",
          "version": "11",
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "OS X 10.13",
          "browserName": "safari",
          "version": ["latest-1", "latest"],
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "OS X 10.13",
          "browserName": "iphone",
          "version": ["11.3", "12.0"],
          "fixSessionCapabilities": "no-detect"
        },
        {
          "platform": "Linux",
          "browserName": "android",
          "version": "6.0",
          "fixSessionCapabilities": "no-detect"
        }
      ]
    },
    "ci-public": {},
    "ci": {
      "extends": ["sauce"]
    }
  }
}
