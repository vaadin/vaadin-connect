{
  "name": "vaadin-connect-demo",
  "version": "0.0.0",
  "description": "Vaadin Connect demo application",
  "author": "Vaadin Ltd.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:frontend": "webpack",
    "start:backend": "node scripts/start/backend.js",
    "start:webpack": "webpack --mode development -w",
    "start:apibrowser": "node scripts/start/apibrowser.js",
    "start": "node scripts/start/backend.js -- node scripts/start/apibrowser.js -- yarn start:webpack",
    "test:e2e": "intern",
    "test": "yarn lint && yarn build:frontend && yarn bundlesize && yarn start:backend -- -- intern",
    "lint": "eslint frontend/ scripts/ e2e/"
  },
  "dependencies": {
    "@ungap/url-search-params": "^0.1.2",
    "@vaadin/connect": "*",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "abortcontroller-polyfill": "^1.2.1",
    "unfetch": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "bundlesize": "^0.17.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.9.0",
    "eslint-config-vaadin": "^0.2.7",
    "html-webpack-plugin": "^3.2.0",
    "intern": "^4.3.3",
    "regenerator-runtime": "^0.13.1",
    "swagger-ui-dist": "^3.20.2",
    "webpack": "^4.28.0",
    "webpack-babel-multi-target-plugin": "^2.0.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10"
  },
  "bundlesize": [
    {
      "path": "./target/classes/META-INF/resources/index.js",
      "maxSize": "2.6 kB"
    }
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "sourceMaps": true
  }
}
