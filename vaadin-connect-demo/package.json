  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:frontend": "connect-scripts build:frontend",
    "lint": "eslint webpack.config.js && tslint 'frontend/**/*.ts' 'e2e/**/*.ts' -e 'frontend/generated/**/*.ts'",
