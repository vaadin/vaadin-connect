    "typescript": "^3.3.3",
    "webpack": "^4.28.0",
    "webpack-babel-multi-target-plugin": "2.1.0-alpha.7",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10"
  },
  "bundlesize": [
    {
      "path": "./target/classes/META-INF/resources/index.js",
      "maxSize": "2.9 kB"
    }
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
